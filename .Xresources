! Transparency
urxvt.depth: 32
urxvt.background: rgba:0000/0000/0200/f000

! Symbols (text)
URxvt.background: black
URxvt.foreground: white

! Settings
URxvt.scrollBar: false
!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull
URxvt.saveLines:  5000

! Font
URxvt.font:xft:System San Francisco Display:size=10:antialias=true
URxvt.letterSpace: -2
